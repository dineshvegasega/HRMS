    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textEmployeeInformationTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/_2C2C2C"
        android:textSize="@dimen/_14sdp"
        android:layout_marginHorizontal="@dimen/_10sdp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        app:fontFamily="@font/roboto_medium"
        android:text="@string/employeesInformation"
        android:visibility="visible"
        android:layout_marginTop="@dimen/_15sdp"
        />


    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/startContract"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/textEmployeeInformationTxt"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginStart="@dimen/_10sdp"
        android:orientation="horizontal"
        android:visibility="visible"
        android:gravity="center_vertical">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textStartDateTxt"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_15sdp"
            android:text="Start Contract:"
            android:textColor="@color/_79747E"
            app:fontFamily="@font/roboto_regular"
            tools:ignore="HardcodedText"
            android:autoSizeTextType="uniform"
            android:autoSizePresetSizes="@array/autosize_4_5_10"
            android:autoSizeStepGranularity="@dimen/_1sdp"
            android:gravity="center_vertical"
            android:textSize="@dimen/_12sdp"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textStartDateValue"
            android:layout_width="@dimen/_57sdp"
            android:layout_height="@dimen/_15sdp"
            android:text="12 Sept, 2023"
            android:gravity="center_vertical"
            android:textColor="@color/black"
            app:fontFamily="@font/roboto_bold"
            tools:ignore="HardcodedText"
            android:textSize="@dimen/_12sdp"
            app:layout_constraintTop_toBottomOf="@+id/textDesc"
            app:layout_constraintStart_toStartOf="parent"
            android:autoSizeTextType="uniform"
            android:autoSizePresetSizes="@array/autosize_4_5_10"
            android:autoSizeStepGranularity="@dimen/_1sdp"/>


    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/endContract"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
